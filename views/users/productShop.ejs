                                            </div>
                                        </div>
                        
                                        <div class="product-content-wrap">
                                            <div class="product-category">
                                                <% if (product[i].productCategory) { %>
                                                    <a href="/productDetailPage?id=<%= product[i]._id %>"><%= product[i].productCategory.catName %></a>
                                                <% } else { %>
                                                    <span>No category available</span>
                                                <% } %>
                                            </div>
                        
                                            <h2><a href="/productDetailPage?id=<%= product[i]._id %>"><%= product[i].productName %></a></h2>
                        
                                            <% if (product[i].countStock == 0) { %>
                                                <div class="product-price">
                                                    <span style="display: inline-block; padding: 2px 16px; background-color: #ff0000; color: #ffffff; font-weight: bold; border-radius: 4px;">Currently Unavailable</span>
                                                </div>
                                            <% } else { %>
                                                <% if (product[i].productCategory && product[i].productCategory.OfferisActive && product[i].offerPrice < product[i].productPrice) { %>
                                                    <div class="product-price">
                                                        <span>₹<%= product[i].offerPrice %></span>
                                                        <span class="old-price" style="color: #ff0000;">₹<%= product[i].productPrice %></span>
                                                
                                                        <span><%= product[i].productCategory.offer %>%Off</span>
                                                    </div>
                                                <% } else { %>
                                                    <div class="product-price">
                                                        <span>₹<%= product[i].productPrice %></span>
                                                    </div>
                                                <% } %>
                                            <% } %>
                        
                                            <div class="product-stock">
                                                <span>Stock: <%= product[i].countStock %></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                        </div> <!-- End of products row -->
                        
                        <% } else { %>
                            <p>No products available.</p> <!-- Message for no products -->
                        <% } %>
                        </div>
                        </div>
                        
        
    </section>
