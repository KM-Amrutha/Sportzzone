    <script>
        function addToCart(productId) {
            event.preventDefault(); // Prevent the default form submission behavior
        
            fetch('/addtoCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    productId
                })
            })
            .then(response => {
                console.log('Fetch response status:', response.status); // Debugging line
        
                if (!response.ok) {
                    throw new Error(`Failed to add item to cart. Status: ${response.status}`);
                }
                return response.json();
            })
            .then(data => {
                console.log('Fetch response data:', data); // Debugging line
        
                if (!data.success) {
                    toastr.error(data.message); // Display error message if adding to cart fails
                } else {
                    toastr.success(data.message); // Display success message
                }
            })
            .catch(error => {
                console.error('Error:', error);
                toastr.error('An error occurred. Please try again.');
            });
        }
        </script>

